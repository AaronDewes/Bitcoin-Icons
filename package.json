{
  "name": "@bitcoin-design/bitcoin-icons",
  "version": "1.0.0",
  "description": "About",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn build-outline && yarn build-filled && yarn build-react && yarn build-vue && yarn build-raw && yarn install",
    "build-react": "node ./scripts/build.js react",
    "build-vue": "node ./scripts/build.js vue",
    "build-raw": "node ./scripts/build.js raw",
    "build-outline": "rimraf ./outline ./optimized/outline && svgo --config=svgo.outline.js -f ./svg/outline -o ./optimized/outline --pretty --indent=2 && cp -R ./optimized/outline ./outline",
    "build-filled": "rimraf ./filled ./optimized/filled && svgo --config=svgo.filled.js -f ./svg/filled -o ./optimized/filled --pretty --indent=2 && cp -R ./optimized/filled ./filled"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@svgr/core": "^6.2.1",
    "@vue/compiler-dom": "^3.2.31",
    "camelcase": "^6.3.0",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitcoinDesign/Bitcoin-Icons.git"
  },
  "keywords": [
    "bitcoin",
    "icons"
  ],
  "author": "Bitcoin Design Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitcoinDesign/Bitcoin-Icons/issues"
  },
  "homepage": "https://github.com/BitcoinDesign/Bitcoin-Icons#readme",
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "packages/*"
  ]
}
